---
# These values NEED to be changed.
# Also, if you create multiple clusters, you may want to
# manually set these values on each of the clusters
cilium_lb_ippool_start: 192.168.121.20
cilium_lb_ippool_stop: 192.168.121.30

# If you are using a localhost docker registry and want
# your VMs to access it over a tunnel, set the following values
#
# See:
# https://docs.docker.com/registry/deploying/
# https://2matoes.com/blog/deploytoremotedocker
#
docker_localregistry: true
docker_localregistry_host: ipaddress-of-host
docker_localregistry_port: 5000
